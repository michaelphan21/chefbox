<body>
  {{> navbar}}

  <form action="success">
  <p>Do you have a rating of 4.0 or higher?<p>
  <select>
    <option value="blank"> </option>
    <option value="yes">Yes</option>
    <option value="no">No</option>
  </select>

  <p>Do you have at least 100 reviews?<p>
  <select>
  <option value="blank"> </option>
    <option value="yes">Yes</option>
    <option value="no">No</option>
  </select>

  <p>Leave a comment below explaining why you deserve to be promoted to chef standing: <p>
   <textarea id = "myTextArea"rows = "8" cols = "80" required> </textarea>
  <br>
  <br>
  <input type = "submit" value="Submit">
  </form>
  {{> footer}}
  <!-- jQuery -->
  <script src="js/jquery.js"></script>

  <!-- Bootstrap Core JavaScript -->
  <script src="js/bootstrap.min.js"></script>
</body>
